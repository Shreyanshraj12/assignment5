    "def sorted_squares(nums):\n",
    "    \n",
    "    squared_nums = [num * num for num in nums]\n",
    "\n",
    " \n",
    "    squared_nums.sort()\n",
    "\n",
    "    return squared_nums\n",
    "nums = [-4, -1, 0, 3, 10]\n",
    "print(sorted_squares(nums))"
   ]
  }
